{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "clinics-ui"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../node_modules/bootstrap/dist/css/bootstrap.min.css",
        "../node_modules/bootstrap/dist/css/bootstrap-theme.min.css",
        "../node_modules/bootstrap/dist/css/bootstrap-social.css",
        "../node_modules/bootstrap/dist/css/font-awesome.min.css",
        "../node_modules/bootstrap/dist/css/bootstrap-datetimepicker.min.css",
        "styles.css",
        "../node_modules/bootstrap/dist/css/search.css",
        "../node_modules/bootstrap/dist/css/cabinet_search.css",
        "../node_modules/bootstrap/dist/css/medicalCard.css",
        "../node_modules/bootstrap/dist/css/passwordStrength.css",
        "../node_modules/bootstrap/dist/css/chatbox.css",
        "../node_modules/bootstrap/dist/css/profile.css"
      ],
      "scripts": [
        "../node_modules/bootstrap/dist/js/bootstrap-datetimepicker.min.js",
        "assets/js/activeClass.js",
        "assets/js/addDoctorValidate.js",
        "assets/js/autoSpecDocs.js",
        "assets/js/chat.js",
        "assets/js/doctorsPatients.js",
        "assets/js/mainSearch.js",
        "assets/js/map.js",
        "assets/js/matchPassword.js",
        "assets/js/medicalCard.js",
        "assets/js/passwordStrength.js",
        "assets/js/patientDoctors.js",
        "assets/js/profile.js",
        "assets/js/search.js",
        "assets/js/sockjs-0.3.4.js",
        "assets/js/stomp.js",
        "assets/js/zxcvbn.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
